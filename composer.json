{
    "name": "dotherightthing/wpdtrt-contentsections",
    "description": "Split the WordPress content area into sections.",
    "type": "library",
    "keywords": ["html5", "section", "block", "heading", "split"],
    "license": "ISC",
    "authors": [
        {
            "name": "Dan Smith",
            "email": "dev@dotherightthing.co.nz",
            "homepage": "https://github.com/dotherightthing/wpdtrt-contentsections",
            "role": "Developer"
        }
    ],
    "support": {
        "email": "support@dotherightthing.co.nz",
        "issues": "https://github.com/dotherightthing/wpdtrt-contentsections/issues"
    },
    "require": {
        "dotherightthing/wpdtrt-plugin-boilerplate": "^1.4.39"
    },
    "require-dev": {
        "phpdocumentor/phpdocumentor": "2.9.0",
        "phpunit/phpunit": "^5.7",
        "psy/psysh" : "~0.6",
        "wp-cli/wp-cli" : "~0.22",
        "wpackagist-plugin/better-anchor-links": "1.7.5"
    },
    "extras": {
        "require-wp": [
            {
                "name": "Better Anchor Links",
                "host": "wpackagist",
                "repository": "wpackagist-plugin/better-anchor-links",
                "file": "auto-anchor-list.php",
                "description": "Provides the anchor points to split the content at"
            }
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-plugin-boilerplate"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/wpdtrt-debug"
        },
        {
            "type": "vcs",
            "url": "https://github.com/dotherightthing/tgm-plugin-activation"
        },
        {
            "type": "composer",
            "url": "https://wpackagist.org"
        }
    ],
    "config": {
        "vendor-dir": "vendor"
    }
}
